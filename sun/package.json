{
  "name": "vagabond-next",
  "author": "Rodrigo Toledo",
  "description": "A Vagabond theme for Next.js",
  "version": "1.0.0",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-format": "prettier --write .",
    "export": "next export",
    "deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages"
  },
  "keywords": [
    "react",
    "next",
    "charts",
    "twitter",
    "vagabond",
    "manga"
  ],
  "dependencies": {
    "@visx/heatmap": "^2.10.0",
    "@visx/mock-data": "^2.1.2",
    "@visx/scale": "^2.2.2",
    "next": "^12.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/styled-components": "^5.1.25",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
